{% set project = params.project %}
{% set projects = params.projects %}
{% set config = params.config %}
{% set PROJECT = project[config.CONFIG_ID] %}
{% set VARS = project[config.CONFIG_ID].navigation %}
{% set tag = params.tag %}

<nav class="navbar navbar-expand-lg navbar-dark bg-dark">
  <div class="container">
    {# <a class="navbar-brand" href="#">Home</a> #}
    {% for key, event in projects['events'] %}
    <div class="switch__button" data-key="{{key}}" data-index="{{loop.index}}" data-type="events">

      <a class="navbar-brand">
        <img src="{{ event.logo | safe}}" alt="" height="30" class="d-inline-block align-text-top {{event.logoClass}} mx-2">
        {{event.title}}
      </a>
    </div>
  {% endfor %}
    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarSupportedContent">
    </div>
  </div>
</nav>